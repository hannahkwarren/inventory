<h1>All Warehouse Locations</h1>

<% @warehouses.each do |warehouse| %>
  <div class="warehouse-<%= "#{warehouse.id}" %>">
    <ul>
    <li><h3><%= link_to warehouse.name, warehouse_path(warehouse.id), method: :get %></h3></li>
      <p><%= warehouse.address %> <%= warehouse.city %>, <%= warehouse.state %> <%= warehouse.zip %></p>
    </ul>
    <% end %>
  </div>

